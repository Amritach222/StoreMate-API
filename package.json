{
  "name": "storemate-api",
  "version": "1.0.0",
  "description": "Inventory Management System with integrated Point of Sale feature to help retailers/shop owners to manage their inventory and track sales on the shops",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "start:prod": "NODE_ENV=production nodemon server.js",
    "debug": "ndb server.js",
    "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js --out-file bundle.js ",
    "build:js": "parcel build ./public/js/index.js --out-dir ./public/js --out-file bundle.js "
  },
  "keywords": [
    "inventory",
    "inventory",
    "management",
    "system",
    "point",
    "of",
    "sale"
  ],
  "author": "Benson Makau",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.2.2",
    "morgan": "^1.10.0",
    "slugify": "^1.6.6",
    "validator": "^13.9.0"
  }
}
